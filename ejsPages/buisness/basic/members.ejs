<body class="p-3 m-0 border-0 bd-example tableHome" >

    <table class="table table-striped table-bordered">
        <tr>
            <th>first name</th>
            <th>last name</th>
            <th>email</th>
            <th>username</th>
            <th>accout type</th>
          </tr>
    
        <% for(let item of items){ %>
        <tr>
            <th class="fname"> <%= item.firstName %> </th>
            <th class="lname"> <%= item.lastName %> </th>
            <th class="email"> <%= item.email %> </th>
            <th class="username"> <%= item.username %> </th>
            <th class="type"> <%= item.type %> </th>    
        
        </tr>
        <% } %>
    
    </table>
    
    </body>